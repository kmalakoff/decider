#!/bin/bash

tag=dist
namespace=decider
images=($(ls -d images/*))

eval "$(minikube docker-env)"
for path in "${images[@]}"
do
  file=${path##*/}
  docker rmi $(docker images -q $namespace/$file:$tag-pack) -f || true
  docker rmi $(docker images -q $namespace/$file:$tag) -f || true
done
docker rmi $(docker images --quiet --filter "dangling=true") || true
