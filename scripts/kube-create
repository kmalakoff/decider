#!/bin/bash

eval "$(minikube docker-env)"
kubectl create -f deploy/image-command-api-deployment.json
kubectl create -f deploy/image-command-api-service.json
kubectl create -f deploy/image-read-api-deployment.json
kubectl create -f deploy/image-read-api-service.json
kubectl create -f deploy/image-web-deployment.json
kubectl create -f deploy/image-web-service.json
